<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.survey.hzyanglili1.mysurvey">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.edit.MySurveiesActivity"/>
        <activity android:name=".activity.use.SurveyListActivity"/>

        <activity android:name=".activity.edit.NewSurveyActivity"/>
        <!--singtop启动-->
        <activity android:name=".activity.edit.StartSurveyActivity"
            >
        </activity>
        <activity android:name=".activity.edit.QuestionSelectActivity"/>
        <activity android:name=".activity.edit.XuanZeQActivity"/>
        <activity android:name=".activity.edit.TiankongQuesActivity"/>
        <activity android:name=".activity.edit.ChengduQuestionActivity"/>

        <activity android:name=".activity.edit.SurveyPrelookActivity"/>

        <activity android:name=".activity.edit.ShowImageActivity"/>

        <activity android:name=".activity.edit.PrelookShowImageActivity"/>
        <activity android:name=".activity.use.ResultListActivity"/>
        <activity android:name=".activity.edit.ZoomImageActivity"/>

    </application>

</manifest>